{
  "name": "react-material-starter",
  "version": "1.0.0",
  "description": "ReactJS + MaterialUI starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-material-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-material-starter/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "cross-env NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --recursive",
    "dev": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "start": "cross-env NODE_ENV=dev node ./server.hot.reload.js",
    "serve-dev": "npm run dev && nodemon ./src/server/index.js",
    "serve-prod": "npm run prod && node ./src/server/index.js"
  },
  "keywords": [
    "material",
    "design",
    "material ui",
    "react",
    "ES6",
    "ES2015",
    "starter",
    "webpack",
    "hot-reload",
    "react-hot-reload"
  ],
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^5.0.0",
    "css-loader": "^0.26.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.0",
    "eslint-plugin-react": "^6.7.1",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.46.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
